{
   "name": "frago9876543210/forms",
   "require-dev": {
      "phpstan/phpstan": "^1.10",
      "pocketmine/pocketmine-mp": "5.0.0",
      "phpstan/phpstan-strict-rules": "^1.0",
      "phpstan/extension-installer": "^1.0"
   },
   "autoload": {
      "psr-4": {
         "": "src/"
      }
   },
   "config": {
      "allow-plugins": {
         "phpstan/extension-installer": true
      }
   },
   "extra":{
      "virion": {
         "spec": "3.0",
         "namespace-root": "forms"
      }
   }
}
